{
  "name": "bpl-node",
  "version": "0.6.1",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "doc": "docco -t docs/template/arkio.jst -c docs/template/arkio.css modules/* app.js README.md",
    "start:bpltestnet": "node app.js --genesis genesisBlock.testnet.json --config config.testnet.json",
    "start:bpldevnet": "node app.js --genesis genesisBlock.devnet.json --config config.devnet.json",
    "start:bplmainnet": "node app.js --genesis genesisBlock.mainnet.json --config config.mainnet.json",
    "start:test": "node app.js --genesis test/genesisBlock.devnet.json --config test/config.devnet.json",
    "test": "./node_modules/.bin/mocha",
    "cov": "./node_modules/.bin/mocha --require blanket -R html-cov test/helpers test/logic > tmp/coverage.html"
  },
  "author": "Raj Singh <rsingh@blockpool.io> ,Boris Povod <boris@crypti.me>, Pavel Nekrasov <landgraf.paul@gmail.com>, Oliver Beddows <oliver@lisk.io>, FX Thoorens <fx.thoorens@ark.io>",
